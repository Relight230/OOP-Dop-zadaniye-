class Nikola:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def get_name(self):
        if name != "Nikola":
            print(f"My name is not {name}, my name is Nikola")
        else:
            print(f"My name is {name}")

name = input("Enter name: ")
age = int(input("Enter age: "))
nikola = Nikola(name, age)
nikola.get_name()
