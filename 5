class KgToPounds:
    def __init__(self, kg):
        self._kg = kg

    @property
    def kg(self):
        return self._kg

    @kg.setter
    def kg(self, value):
        self._kg = value

    def to_pounds(self):
        return self._kg * 2.202462

kg = float(input("Enter your Kg: "))
weight = KgToPounds(kg)
print("Weight: ", weight.to_pounds())
