class Animal:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def info(self):
        print(f"Имя: {self.name}\nAge: {self.age}")

    def Speak(self):
        pass

class Dog(Animal):
    def Speak(self):
        print("Gau gau")

class Cat(Animal):
    def Speak(self):
        print("Myau myau")

dog = Dog("Luntik", 7)
dog.info()
dog.Speak()
cat = Cat("Dobbi", 3)
cat.info()
cat.Speak()
